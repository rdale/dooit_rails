<h1>New vtodo</h1>

<% form_for(@vtodo) do |f| %>

  <p>
    <%= f.label :summary %><br />
    <%= f.text_field :summary %>
  </p>
  <p>
    <%= f.label :notes %><br />
    <%= f.text_field :description %>
  </p>
	<!--insert tag-->
  <p>
	<%= f.label :tags %><br />
	<%= f.text_field :tags %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', vtodos_path %>
